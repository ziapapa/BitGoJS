{
  "name": "bitgo",
  "version": "5.0.0",
  "description": "BitGo Javascript SDK",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "keywords": [
    "bitgo",
    "bitcoin",
    "bitgojs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BitGo/BitGoJS.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.12.3 <12.0.0",
    "npm": ">=3.10.10"
  },
  "scripts": {
    "coverage": "./node_modules/.bin/nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "babelify": "mkdir -p browser && ./node_modules/.bin/browserify ./dist/src/index.js --exclude superagent-proxy --exclude morgan -t babelify -s BitGoJS -o browser/BitGoJS-babelify.js",
    "browserify-sjcl-bitgo": "./node_modules/.bin/browserify ./example/sjcl_bitgo.js -s SJCLBitGo -o ./sjcl-bitgo.js",
    "compile": "webpack --progress --colors --env.prod",
    "compile-dbg": "webpack --progress --colors --env.dev",
    "compile-test": "webpack --progress --colors --env.test",
    "test": "npm run test-node",
    "express": "node bin/bitgo-express -b 0.0.0.0",
    "test-node": "./node_modules/.bin/nyc -- ./node_modules/.bin/_mocha -r ts-node/register --timeout 20000 --reporter list \"test/unit/*.ts\" \"test/unit/**/*.ts\" \"test/v2/unit/*.ts\" \"test/v2/unit/**/*.ts\"",
    "test-browser": "npm run compile-test && ./node_modules/karma/bin/karma start .karma.conf.js --log-level debug --single-run",
    "test-integration": "./node_modules/.bin/_mocha -r ts-node/register --timeout 20000 --reporter list \"test/integration/*.ts\" \"test/integration/**/*.ts\" \"test/v2/integration/*.js\" \"test/v2/integration/**/*.ts\"",
    "test-travis": "bash test/scripts/ci.sh",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "test-integration-utxo-wallet": "./node_modules/.bin/mocha --require ts-node/register --reporter list --recursive ./test/v2/integration/utxoWallet/",
    "audit": "if [ \"$(npm --version | cut -d. -f1)\" -ge \"6\" ]; then npm audit; else echo \"npm >= 6 required to perform audit. skipping...\"; fi",
    "generate_drone": "drone jsonnet --stream",
    "clean": "rm -r dist/*",
    "postinstall": "tsc && mkdir -p ./dist/src/vendor && cp ./src/vendor/sjcl.min.js ./dist/src/vendor"
  },
  "dependencies": {
    "@bitgo/unspents": "0.5.0",
    "@types/lodash": "4.14.121",
    "@types/node": "11.11.4",
    "argparse": "1.0.10",
    "assert": "0.4.9",
    "big.js": "3.1.3",
    "bigi": "1.4.0",
    "bignumber.js": "8.0.1",
    "bitcoinjs-message": "2.0.0",
    "bitgo-utxo-lib": "1.3.1",
    "bluebird": "3.5.3",
    "body-parser": "1.18.3",
    "bs58": "2.0.1",
    "bs58check": "1.0.4",
    "cashaddress": "1.1.0",
    "create-hmac": "1.1.7",
    "debug": "3.1.0",
    "ecurve": "1.0.6",
    "eol": "0.5.0",
    "express": "4.16.4",
    "http-proxy": "1.11.1",
    "lodash": "4.17.11",
    "minimist": "0.2.0",
    "moment": "2.20.1",
    "morgan": "1.9.1",
    "prova-lib": "0.2.10",
    "ripple-lib": "0.22.0",
    "sanitize-html": "1.13.0",
    "secrets.js-grempe": "1.1.0",
    "stellar-sdk": "0.11.0",
    "superagent": "3.8.3",
    "superagent-proxy": "1.0.3"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.25",
    "@types/mocha": "5.2.6",
    "@types/nock": "9.3.1",
    "@types/sinon": "7.0.6",
    "@typescript-eslint/eslint-plugin": "1.4.2",
    "@typescript-eslint/parser": "1.4.2",
    "awesome-typescript-loader": "5.2.1",
    "browserify": "16.2.3",
    "bufferutil": "3.0.5",
    "eslint": "5.15.1",
    "eslint-config-prettier": "4.1.0",
    "glob": "7.1.3",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.4",
    "mocha": "4.1.0",
    "mocha-junit-reporter": "1.17.0",
    "mocha-lcov-reporter": "0.0.2",
    "nock": "9.0.28",
    "nyc": "13.3.0",
    "precise-commits": "1.0.2",
    "prettier": "1.16.3",
    "q": "1.1.2",
    "request": "2.88.0",
    "request-promise": "4.2.2",
    "should": "13.1.3",
    "should-http": "0.1.1",
    "should-sinon": "0.0.6",
    "sinon": "6.3.5",
    "sjcl": "1.0.8",
    "supertest": "3.0.0",
    "supertest-as-promised": "https://github.com/BitGo/supertest-as-promised/archive/a7f4b612b9fa090ae33a9616c41862aec2b25c7e.tar.gz",
    "terser": "3.16.1",
    "terser-webpack-plugin": "1.2.3",
    "ts-node": "8.0.2",
    "typescript": "3.3.4000",
    "utf-8-validate": "4.0.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3"
  },
  "optionalDependencies": {
    "ethereumjs-abi": "0.6.5",
    "ethereumjs-tx": "1.3.7",
    "ethereumjs-util": "4.4.1",
    "secp256k1": "3.6.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,js,json}": [
      "precise-commits",
      "git add"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  }
}
